{
  "env": {
    "development": {
      "application/javascript": {
        "presets": [
          [
            "env",
            {
              "targets": {
                "electron": "1.4.15"
              }
            }
          ],
          "react"
        ],
        "plugins": [
          "transform-async-to-generator"
        ],
        "sourceMaps": "inline"
      },
      "text/typescript": {
        "allowSyntheticDefaultImports": true,
        "removeComments": false,
        "preserveConstEnums": true,
        "declaration": true,
        "strict": true,
        "inlineSourceMap": true,
        "inlineSources": true,
        "noUnusedParameters": true,
        "importHelpers": true,
        "noEmitHelpers": true,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "module": "commonjs",
        "moduleResolution": "node",
        "target": "es2016",
        "jsx": "react",
        "typeRoots": [ "./types", "./@types", "./node_modules/@types"],
        "baseUrl": ".",
        "paths": {
          "*": ["./types/*"]
        }
      }
    },
    "production": {
      "application/javascript": {
        "presets": [
          [
            "env",
            {
              "targets": {
                "electron": "1.4.15"
              }
            }
          ],
          "react"
        ],
        "plugins": [
          "transform-async-to-generator"
        ],
        "sourceMaps": "none"
      },
      "text/typescript": {
        "removeComments": false,
        "preserveConstEnums": true,
        "declaration": true,
        "strict": true,
        "sourceMap": true,
        "importHelpers": true,
        "noEmitHelpers": true,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "target": "es2016",
        "jsx": "react",
        "typeRoots": [ "./types", "./@types", "./node_modules/@types"],
        "baseUrl": ".",
        "paths": {
          "*": ["./types/*"]
        }
      }
    }
  }
}
